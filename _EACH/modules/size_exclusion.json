{
  "size_exclusion": {
    "id": "size_exclusion",
    "title": "Size Exclusion Chromatography",
    "subtitle": "Compare outcomes across multiple SEC columns",
    "icon": "icons/modules/placeholder.svg",
    "settings": {
      "Fractionation mode": {
        "formtype": "ChoiceField",
        "options": {
          "Keep proteins within column fractionation range": "inside",
          "Remove proteins within column fractionation range": "outside"
